#!/usr/bin/env node

'use strict';

var _core = require('puck-lang/dist/lib/stdlib/core');

var _helpers = require('./../lib/helpers');

var _test = require('./../lib/test');

(0, _test.buildTestCompiler)();
(0, _test.buildAll)(".tmp/new", ".tmp/old");
(0, _helpers.cmd)("diff --exclude=node_modules -Nr .tmp/old .tmp/new");
(0, _core.print)("done");
