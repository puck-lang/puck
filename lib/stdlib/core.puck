//#![no_core]
import 'puck:js' as {undefined, console}

export type Bool = {}
export type Num = {}
export type String = {}

export type List<E> = {}

export fn print(message, a, b, c) {
  let mut args = [message]
  if a != undefined {
    args.push(a)
  }
  if b != undefined {
    args[2] = b
  }
  if c != undefined {
    args[3] = c
  }

  console.log.apply(console, args)
}
