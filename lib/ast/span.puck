
export type Position {
  line: Num
  column: Num
}

export type Span {
  start: Position
  end: Position
}

impl Span {
  fn empty() -> Span {{
    start: {line: 0, column: 0}
    end: {line: 0, column: 0}
  }}
}

export trait ToSpan {
  fn span(self) -> Span
}

impl ToSpan for Span {
  fn span(self) -> Span {self}
}
